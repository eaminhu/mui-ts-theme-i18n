{
    "name": "passto-creditcard",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "pm2": "npm run build && npm run pm2-first && npm run pm2-bak",
        "pm2-first": "pm2 start pm2.json --env production --only passto",
        "pm2-bak": "pm2 start pm2.json --env production --only passto-bak",
        "lint": "npx eslint --ext .js,.ts,.jsx,.tsx .",
        "lint:fix": "npx eslint --fix --ext .js,.ts,.jsx,.tsx .",
        "stylelint:fix": "stylelint \"./**/*.{css,scss}\" --fix",
        "prettier": "prettier --write '**/*.{js,ts,jsx,tsx,scss,md,json}'",
        "prepare": "husky install",
        "commit": "git-cz"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "npm run lint"
        ],
        "*.{css,scss}": [
            "npm run stylelint:fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
